{
  "builds": [{
    "src": "GMI/wsgi.py",
    "use": "@vercel/python",
    "config": {
      "maxLambdaSize": "15mb",
      "runtime": "python3.11"
    }
  }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "GMI/wsgi.py"
    }
  ],
  "functions": {
    "api": {
      "include": "api/**/*.py"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/vercel/output/\\1"
    }
  ]
}
